# API Keys and Configuration
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/nonprofit_db
REDIS_URL=redis://localhost:6379
MONGODB_URI=mongodb://localhost:27017/nonprofit_data

# IRS Data Access
IRS_990_API_KEY=your_api_key_here
IRS_DATA_URL=https://www.irs.gov/pub/irs-soi/

# Social Media APIs
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token

# OpenAI for NLP
OPENAI_API_KEY=your_openai_api_key

# Geocoding
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Web Scraping
USER_AGENT=Mozilla/5.0 (compatible; NonprofitAnalyzer/1.0)
SCRAPE_DELAY=2  # seconds between requests

# Application Settings
APP_ENV=development
APP_PORT=8000
APP_HOST=0.0.0.0
LOG_LEVEL=INFO

# Cache Settings
CACHE_TTL=86400  # 24 hours in seconds
MAX_CACHE_SIZE=1000  # Maximum number of cached items